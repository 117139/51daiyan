						<view>¥500</view>
					</view>
					<view  class="pp_ph_li">
						<view>三等奖</view>
						<view>第26～60名</view>
						<view>¥300</view>
					</view>
				</view>
			</view>
			<view class="pp_tip">
				<view class="tip_text">活动起止时间：2019年1月1日</view>
				<view class="tip_btn">报名</view>
			</view>
		</view>
		<view class="xx_box1 xx_box2">
			<view class="pp_v1">
				<view class="pp_img">
					<image class="pp_img" src="../../static/images/tx.png"></image>
