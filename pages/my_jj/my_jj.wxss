/* pages/my_name/my_name.wxss */
page{
	overflow-y: auto!important;
	background: #fff;
}
.container{
	width: 750rpx;
	min-height: 100vh;
	
	background: #fff;
  padding: 0 28rpx;
}
.set_name{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between; 
  min-height: 95rpx;
  border-bottom: 1px solid #eee;
}
.set_l{
  font-size: 30rpx;
  color: #333;
}
.set_btn{
  font-size: 30rpx;
  color: #FE8735;
  
}
.set_name textarea{
  width: 100%;  
  height:280rpx;
  background:rgba(238,238,238,1);
  border-radius:10rpx;
  padding: 20rpx;
  box-sizing: border-box;
}